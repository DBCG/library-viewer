<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER_LEFT" prefHeight="747.0" prefWidth="709.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.alphora.cql.ViewerController">
   <children>
      <Label text="Status" />
      <TextField fx:id="status" disable="true" />
      <Label text="URL" />
      <TextField fx:id="url" />
      <Button fx:id="get" onAction="#get" text="GET" />
      <Label text="CQL" />
      <TextArea fx:id="cql" prefHeight="493.0" prefWidth="669.0" />
      <Button fx:id="put" onAction="#put" text="PUT" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
